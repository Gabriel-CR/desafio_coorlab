<img src=\"../assets/logo.png\" alt=\"Logo da empresa\" />\n        <b>{{ appName }}</b>\n      </b-navbar-brand>\n    </b-navbar>\n    <div>\n      <h1>Insira o destino e o peso</h1>\n      <!-- input de destino -->\n      <b-form-group id=\"input-group-1\" label=\"Destino\" label-for=\"input-1\">\n        <!-- input dropdown -->\n        <b-form-select\n          id=\"input-1\"\n          v-model=\"destino\"\n          :options=\"options\"\n          required\n          placeholder=\"Selecione o destino\"\n        ></b-form-select>\n      </b-form-group>\n      <!-- input de peso -->\n      <b-form-group id=\"input-group-2\" label=\"Peso\" label-for=\"input-2\">\n        <b-form-input\n          id=\"input-2\"\n          v-model=\"peso\"\n          type=\"text\"\n          placeholder=\"300 kg\"\n        ></b-form-input>\n      </b-form-group>\n    </div>\n    <h1>Output</h1>\n  </div>\n</template>\n\n<script>\nimport { BFormInput, BFormGroup, BFormSelect } from \"bootstrap-vue\";\n\nexport default {\n  components: {\n    BNavbar,\n    BNavbarBrand,\n  },\n  data() {\n    const appName = \"\";\n    const cidades = [];\n\n    return {\n      appName,\n      cidades,\n    };\n  },\n  created() {\n    // Implemente aqui o GET dos dados da API REST\n    // para que isso ocorra na inicialização da pagina\n\n    fetch(\"http://localhost:3000/transport\")\n      .then((res) => res.json())\n      .then((data) => {\n        this.cidades = data;\n      })\n      .catch((err) => {\n        console.log(err);\n      });\n\n    this.appName = \"Melhor Frete\";\n  },\n  methods: {\n    // Implemente aqui os metodos utilizados na pagina\n    methodFoo() {\n      console.log(this.appName);\n    },\n    methodData() {\n      console.log(this.cidades);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.title .navbar {\n  background-color: #00aca6 !important;\n}\n\n.title .navbar-brand {\n  margin-left: 20px;\n}\n\n.title .navbar-brand img {\n  margin-right: 10px;\n  width: 30px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}